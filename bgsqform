    <script type="text/javascript">

        $(function () {
            /*focus*/
            var sliderElement = $('.slider-carousel');
            sliderElement.slidesjs({
                play: {
                    auto: true,
                    effect: 'fade'
                },
                navigation: {
                    effect: "fade"
                },
                pagination: {
                    effect: "fade"
                },
                effect: {
                    slide: {
                        speed: 400
                    }
                }
            });
            var slidernav = sliderElement.find('.slidesjs-navigation');
            sliderElement.hover(function () {
                slidernav.stop().show()
            }, function () {
                slidernav.stop().hide()
            });

            /*要闻*/
            $(".index_focus .rig_list ul li").hover(function () {
                $(this).toggleClass("hover")
            })

            /*Tab*/
            $(".index_tab").each(function () {
                $(this).find(".tabmenu li").mouseover(function () {
                    index = $(".index_tab .tabmenu li").index(this);
                    $(this).addClass("selected").siblings().removeClass("selected");
                    $(".tabox .tab_con").eq(index).show().siblings().hide();
                });
            })
            $(".index_tab .index_list01 li").each(function () {
                $(this).width(function () {
                    return $(this).width();
                })
            })

            //回应关切
            $(".index_hygq ul li").each(function () {
                index = $(".index_hygq ul li").index(this);
                $(this).attr("class", "icos" + index)
                $(this).hover(function () {
                    $(this).addClass("hover").siblings().removeClass("hover")
                }, function () {
                    $(this).removeClass("hover")
                })
            })
            $(".index_hygq ul li:eq(0)").addClass("hover")

            //ico
            $(".index_tab .fwcx .fw ul li").each(function () {
                $(".index_tab .fwcx .fw ul li").hover(function () {
                    $(this).find("img").stop().animate({
                        "margin-top": 0
                    })
                }, function () {
                    $(this).find("img").stop().animate({
                        "margin-top": "5px"
                    })
                })
            })
            //
            $('#demo01').flexslider({
                slideshow: true,
                animation: "slide",
                direction: "horizontal",
                easing: "swing",
                controlNav: false,
                directionNav: true
            });
            $('#demo02').flexslider({
                slideshow: true,
                animation: "slide",
                direction: "horizontal",
                easing: "swing",
                controlNav: false,
                directionNav: true
            });
            $(".flexslider").each(function () {
                $(this).hover(function () {
                    $(this).find(".flex-direction-nav").fadeIn("slow")
                }, function () {
                    $(this).find(".flex-direction-nav").hide()
                })
            })
        })

    </script>
